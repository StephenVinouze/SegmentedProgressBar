<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SegmentedProgressBar">
        <attr name="spb_count" format="integer" />
        <attr name="spb_duration" format="integer" />
        <attr name="spb_segmentAlpha" format="float" />
        <attr name="spb_progressAlpha" format="float" />
        <attr name="spb_segmentColor" format="color" />
        <attr name="spb_progressColor" format="color" />
        <attr name="spb_spacing" format="dimension" />
        <attr name="spb_angle" format="integer" />
        <attr name="spb_enableLastProgressionAnimation" format="boolean" />
    </declare-styleable>

</resources>